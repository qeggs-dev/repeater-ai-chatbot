{
    "api_info": {
        "api_file_path": "./configs/api_info.json",
        "default_model_uid": "chat",
        "case_sensitive": false
    },
    "blacklist": {
        "file_path": "./configs/blacklist.regex",
        "match_timeout": 10.0
    },
    "callapi": {
        "max_concurrency": 1000,
        "include_usage": true,
        "include_obfuscation": false
    },
    "context": {
        "auto_shrink_length": null,
        "save_context": true,
        "save_text_only": true,
        "max_log_length_for_non_text_content": 25
    },
    "logger": {
        "file_path": "./logs/repeater-log-{time:YYYY-MM-DD_HH-mm-ss.SSS}.log",
        "level": "DEBUG",
        "rotation": "10 MB",
        "retention": "7 days",
        "compression": "zip"
    },
    "model": {
        "default_temperature": 1.0,
        "default_top_p": 1.0,
        "default_max_tokens": 4096,
        "default_max_completion_tokens": 4096,
        "default_frequency_penalty": 0.0,
        "default_presence_penalty": 0.0,
        "default_stop": [],
        "stream": true
    },
    "prompt_template": {
        "version": "",
        "bot_info": {
            "name": "Repeater",
            "birthday": {
                "day": 28,
                "month": 6,
                "year": 2024
            }
        },
        "time": {
            "timezone": "Asia/Shanghai"
        }
    },
    "prompt": {
        "dir": "./configs/prompt/presets",
        "suffix": ".md",
        "encoding": "utf-8",
        "preset_name": "repeater",
        "load_prompt": true
    },
    "render": {
        "default_image_timeout": 60.0,
        "markdown": {
            "default_style": "light",
            "styles_dir": "./configs/styles",
            "style_file_encoding": "utf-8",
            "html_template_dir": "./configs/html_templates",
            "html_template_file_encoding": "utf-8",
            "default_html_template": "default",
            "html_template_suffix": ".html",
            "preprocess_map": {
                "before": {},
                "after": {}
            },
            "title": "Repeater Image Generator"
        },
        "to_image": {
            "max_pages_per_browser": 5,
            "max_browsers": 2,
            "browser_type": "",
            "headless": true,
            "output_dir": "./workspace/temp/render",
            "output_suffix": ".png",
            "executable_path": "",
            "width": 1200,
            "height": 600
        }
    },
    "request_log": {
        "dir": "./workspace/request_log",
        "auto_save": true,
        "debonce_save_wait_time": 1200.0,
        "max_cache_size": 1000
    },
    "server": {
        "host": null,
        "port": null,
        "workers": null,
        "reload": null
    },
    "static": {
        "readme_file_path": "./README.md",
        "static_dir": "./static"
    },
    "user_config_cache": {
        "downgrade_wait_time": 600.0,
        "debounce_save_wait_time": 1000.0
    },
    "user_data": {
        "dir": "./workspace/data/user_data",
        "branches_dir_name": "branches",
        "metadata_file_name": "metadata.json",
        "cache_medadata": false,
        "cache_data": false
    },
    "user_nickname_mapping": {
        "file_path": "./configs/user_nickname_mapping.json"
    },
    "web": {
        "index_web_file": ""
    }
}